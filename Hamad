function inGame()
  inGameOption1 = gg.choice({
    "🇰🇼	 Wallhack & Colors",
    "🇰🇼	 All Functions",
    "🇰🇼	 Back",}, nil, "[MENU] Game")
  if inGameOption1 == 1 then inGameWallColors() end
  if inGameOption1 == 2 then inGameAll() end
  if inGameOption1 == 3 then HOME() end
end

