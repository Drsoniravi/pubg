HOMEDM = 1
function HOME()
  menuOption = gg.choice({"🇰🇼	[MENU] Lobby🇰🇼	",
    "🇰🇼	[MENU] Game🇰🇼	",
    "Close",
  }, nil, "Hamad's VIP Script (PUBGMHACKS.ONLINE)")

  if menuOption == 1 then
    inLobby()
  end
  if menuOption == 2 then
    inGame()
	end
  if menuOption == 3 then
    exitAndPrint()
  end
  HOMEDM = -1
end


